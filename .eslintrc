{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint",
    "unused-imports"
  ],
  "extends": [
    "prettier"
  ],
  "rules": {
    "semi": "error",
    "@typescript-eslint/consistent-type-imports": "error",
    "unused-imports/no-unused-imports": "error",
    "no-trailing-spaces": [
      "error",
      {
        "ignoreComments": true
      }
    ]
  }
}